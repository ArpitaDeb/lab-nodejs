<!doctype html>
<html>
<head>
  <title>Test'em</title>
  <link rel="stylesheet" href="/testem/jasmine.css">
</head>
<body>
  <div><p>&nbsp;&nbsp;&nbsp;&nbsp;Test'em : custom page : Jasmine + JQuery</p></div>


  <div id="jasmine_content"></div>

  <!-- js scripts -->

  <script src="/testem/jasmine.js"></script>
  <script src="/testem.js"></script>
  <script src="/testem/jasmine-html.js"></script>

  <script src="/bower_components/jquery/dist/jquery.min.js"></script>

  {{#serve_files}}<script src="{{src}}"{{#attrs}} {{&.}} {{/attrs}}></script>{{/serve_files}}

  <script type="text/javascript">
  (function() {
    'use strict';

    var jasmineEnv = jasmine.getEnv();
    jasmineEnv.addReporter(new jasmine.HtmlReporter);

    $( document ).ready(function() {
      console.log( "document ready!" );
      console.log( "Execute Jasmine..." );
      jasmineEnv.execute();
    });

  })();
  </script>
</body>
</html>
